{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh3/main/themes/schema.json",
    "final_space": true,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{.User}}@{{.Host}}{{if .Root}} as root{{end}} in {{.Path}}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "diamond",
                    "foreground": "#000000",
                    "background": "#D3D7CF",
                    "leading_diamond": "╭─",
                    "properties": {}
                },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#111111",
                    "background": "#FFFF66",
                    "properties": {
                        "root_icon": ""
                    }
                },
                {
                    "type": "session",
                    "style": "powerline",
                    "foreground": "#D4E157",
                    "background": "#535353",
                    "powerline_symbol": ""
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#D3D7CF",
                    "background": "#3465A4",
                    "properties": {
                        "prefix": " \uF07C ",
                        "home_icon": "~",
                        "style": "full"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "foreground": "#000000",
                    "background": "#4E9A06",
                    "properties": {
                        "branch_icon": "\uF126 ",
                        "display_stash_count": true,
                        "display_upstream_icon": true,
                        "status_colors_enabled": true,
                        "local_changes_color": "#C4A000",
                        "ahead_and_behind_color": "#F26D50",
                        "behind_color": "#4E9A06",
                        "ahead_color": "#89D1DC",
                        "stash_count_icon": "\uF692 "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#FFFFFF",
                    "background": "#689F63",
                    "properties": {
                        "postfix": "  ",
                        "display_mode": "files",
                        "display_version": true,
                        "enable_hyperlink": true,
                        "display_package_manager": true
                    }
                },
                {
                    "type": "go",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#111111",
                    "background": "#00ACD7",
                    "properties": {
                        "postfix": "  ",
                        "display_mode": "files",
                        "display_version": true,
                        "enable_hyperlink": true
                    }
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#111111",
                    "background": "#FFDE57",
                    "properties": {
                        "postfix": "  ",
                        "display_version": true,
                        "display_mode": "files",
                        "display_virtual_env": true,
                        "enable_hyperlink": true
                    }
                },
                {
                    "type": "ruby",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#FFFFFF",
                    "background": "#AE1401",
                    "properties": {
                        "postfix": "  ",
                        "display_version": true,
                        "display_mode": "files",
                        "enable_hyperlink": true
                    }
                },
                {
                    "type": "terraform",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#000000",
                    "background": "#EBCC34",
                    "properties": {
                        "postfix": " 﫳 ",
                        "template": "{{.WorkspaceName}}"
                    }
                },
                {
                    "type": "executiontime",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#000000",
                    "background": "#C4A000",
                    "properties": {
                        "postfix": " \uF252 "
                    }
                },
                {
                    "type": "exit",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": true,
                    "foreground": "#D3D7CF",
                    "background": "#3B3B3B",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#E91E63{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true,
                        "display_exit_code": true,
                        "error_color": "#CC2222",
                        "color_background": true,
                        "success_icon": ""
                    }
                },
                {
                    "type": "time",
                    "style": "diamond",
                    "invert_powerline": true,
                    "background": "#D3D7CF",
                    "foreground": "#000000",
                    "properties": {
                        "postfix": "  "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#D3D7CF",
                    "properties": {
                        "prefix": "",
                        "postfix": "",
                        "text": "┃"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#D3D7CF",
                    "properties": {
                        "prefix": "",
                        "postfix": "",
                        "text": "╰─ ﬦ"
                    }
                }
            ]
        }
    ]
}